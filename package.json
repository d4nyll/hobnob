{
  "name": "hobnob",
  "version": "0.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "rimraf dist && babel src -d dist --copy-files",
    "fix": "eslint src --fix",
    "lint": "eslint src",
    "precommit": "yarn run lint",
    "serve": "yarn run build && dotenv -e envs/.env node dist/index.js",
    "test": "yarn run test:unit && yarn run test:e2e",
    "test:unit": "mocha 'src/**/*.test.js' --require @babel/register",
    "test:e2e": "dotenv -e envs/test.env -e envs/.env ./scripts/e2e.test.sh",
    "watch": "nodemon -w src --exec yarn run serve"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/d4nyll/hobnob.git"
  },
  "author": "Daniel Li <dan@danyll.com> (http://danyll.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/d4nyll/hobnob/issues"
  },
  "homepage": "https://github.com/d4nyll/hobnob#readme",
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/register": "^7.0.0",
    "cucumber": "^4.2.1",
    "dotenv-cli": "^1.4.0",
    "eslint": "^5.5.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "husky": "^0.14.3",
    "lodash.clonedeep": "^4.5.0",
    "lodash.isequal": "^4.5.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.4",
    "rimraf": "^2.6.2",
    "sinon": "^6.2.0",
    "superagent": "^3.8.3"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "ajv": "^6.5.3",
    "body-parser": "^1.18.3",
    "elasticsearch": "^15.1.1",
    "express": "^4.16.3"
  }
}
